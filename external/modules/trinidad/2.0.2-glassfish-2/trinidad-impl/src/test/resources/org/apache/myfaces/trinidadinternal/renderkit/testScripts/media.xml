<test xmlns="http://myfaces.apache.org/trinidad/test"
       xmlns:tr="http://myfaces.apache.org/trinidad">
  <base-component tr:type="org.apache.myfaces.trinidad.CoreMedia" source="/components/images/seattle.wmv"/>
  
  <boolean-test name="autostart"/>
  <attribute-test name="contentType"/>
  
  <attribute-test name="controls" value="none"/>
  <attribute-test name="controls" value="noneVisible"/>
  <attribute-test name="controls" value="minimal"/>
  <attribute-test name="controls" value="typical" matchesBase="true"/>
  <attribute-test name="controls" value="all"/>
  
  <attribute-test name="innerWidth" value="160">
    <attribute-test name="width" value="190"/>
  </attribute-test>
  <attribute-test name="innerHeight" value="112">
    <attribute-test name="height" value="130"/>
  </attribute-test>
  <attribute-test name="width" value="160"/>
  <attribute-test name="height" value="112"/>
  
  <!-- attribute-test name="playCount" value="1"/-->
  
  <!--attribute-test name="player" value="quicktime"/-->
  <!--attribute-test name="player" value="none"/-->
  <attribute-test name="player" value="real"/>
  <!--attribute-test name="player" value="windows"/-->
  
  <!--attribute-test name="standbyText"/-->
  
  <!-- Core attribute tests -->
  <attribute-test name="id"/>
  <!--attribute-test name="inlineStyle" /-->
  <boolean-test name="rendered"/>
  <attribute-test name="shortDesc"/>
  <!--attribute-test name="styleClass"/-->

  <js-test/>
</test>
